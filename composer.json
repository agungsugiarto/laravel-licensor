{
    "name": "agungsugiarto/laravel-licensor",
    "description": "Laravel licensing system",
    "license": "MIT",
    "homepage": "https://github.com/agungsugiarto/laravel-licensor",
    "keywords": [
        "Laravel",
        "Licensing system"
    ],
    "authors": [
        {
            "name": "Sribna",
            "role": "Developer"
        },
        {
            "name": "Agung Sugiarto",
            "email": "me.agungsugiarto@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "orchestra/testbench": "^5.0|^6.0|^7.0",
        "guzzlehttp/guzzle": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "Fluent\\Licensor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fluent\\Licensor\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fluent\\Licensor\\LicensorServiceProvider"
            ]
        }
    }
}
